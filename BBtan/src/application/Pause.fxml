<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="scene" prefHeight="600.0" prefWidth="600.0" stylesheets="@pause.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PauseController">
   <children>
      <Button fx:id="restartBtn" layoutX="405.0" layoutY="492.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="148.0" text="Restart">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="continueBtn" layoutX="50.0" layoutY="492.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="148.0" text="Continue">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="menuBtn" layoutX="254.0" layoutY="492.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="92.0" text="Menu">
         <font>
            <Font size="21.0" />
         </font></Button>
      <Text fill="#9c3927" layoutX="206.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Paused">
         <font>
            <Font name="Showcard Gothic" size="50.0" />
         </font>
      </Text>
      <Text fill="#655248" layoutX="156.0" layoutY="144.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Take a break!">
         <font>
            <Font name="Showcard Gothic" size="40.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
