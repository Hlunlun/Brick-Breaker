<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GameoverController">
   <children>
      <AnchorPane layoutX="-2.0" prefHeight="400.0" prefWidth="400.0">
         <children>
            <Label layoutX="115.0" layoutY="158.0" text="Your score is" />
            <Label fx:id="score" layoutX="216.0" layoutY="158.0" text="0" />
            <Button fx:id="menuBtn" layoutX="115.0" layoutY="233.0" mnemonicParsing="false" onAction="#goMenu" text="Menu" />
            <Button fx:id="againBtn" layoutX="223.0" layoutY="233.0" mnemonicParsing="false" onAction="#again" text="play again" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
